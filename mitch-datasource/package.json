{
  "name": "mitch-datasource",
  "version": "1.0.0",
  "description": "Datasource component to generalise the way to perform data-related operations such as paging, filtering, sorting, etc.",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "npm-run-all \"lint\" \"build-dev\"",
    "prd": "npm-run-all \"lint\" \"build-prd\"",
    "watch": "npm-run-all --parallel \"watch-lint\" \"watch-webpack\" \"watch-docs\"",
    "build-dev": "npm-run-all \"build-webpack-dev\" \"build-docs\"",
    "build-prd": "npm-run-all \"build-webpack-prd\" \"build-docs\"",
    "build-watch": "npm-run-all --parallel \"watch-webpack\" \"watch-docs\"",
    "watch-webpack": "npm run build-webpack-dev -- --watch",
    "build-webpack-dev": "webpack --config webpack.config.js --mode=development",
    "build-webpack-prd": "webpack --config webpack.config.js --mode=production",
    "lint": "eslint .",
    "lint-watch": "nodemon --watch ./src/js --ext * --exec \"npm run lint-build\"",
    "lint-fix": "npm run lint -- --fix",
    "build-docs": "esdoc -c esdoc.json",
    "watch-docs": "nodemon --watch ./src/js --watch ./manual --ext * --exec \"npm run build-docs\""
  },
  "keywords": [
    "datasource",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deltoss/mitch-datasource.git"
  },
  "author": "Michael Tran <MTran0011@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deltoss/mitch-datasource/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-loader": "^8.0.5",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-publish-markdown-plugin": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.3.6",
    "regenerator-runtime": "^0.13.3",
    "query-string": "^6.8.3"
  }
}
